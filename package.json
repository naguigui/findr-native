{
	"main": "index.js",
	"scripts": {
		"start:dev": "better-npm-run development",
		"start:prod": "better-npm-run production",
		"start": "expo start",
		"android": "expo start --android",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"format": "prettier --write \"**/*.{js,json,md,yaml,yml}\"",
		"lint": "eslint src",
		"generate": "plop",
		"eject": "expo eject",
		"storybook:server": "storybook start -p 7007 -c .storybook/local",
		"storybook:start": "better-npm-run storybook:start"
	},
	"config": {
		"react-native-storybook-loader": {
			"searchDir": [
				"./src/components",
				"./packages"
			],
			"pattern": "**/*.stories.js",
			"outputFile": "./storybook/storyLoader.js"
		}
	},
	"betterScripts": {
		"development": {
			"command": "expo start",
			"env": {
				"REACT_NATIVE_ENVIRONMENT": "development"
			}
		},
		"production": {
			"command": "expo start",
			"env": {
				"REACT_NATIVE_ENVIRONMENT": "production"
			}
		},
		"storybook:start": {
			"command": "rnstl && expo start",
			"env": {
				"REACT_NATIVE_ENVIRONMENT": "storybook"
			}
		}
	},
	"dependencies": {
		"apollo-cache-inmemory": "^1.6.2",
		"apollo-cache-persist": "^0.1.1",
		"apollo-client": "^2.6.3",
		"apollo-link": "^1.2.12",
		"apollo-link-context": "^1.0.18",
		"apollo-link-error": "^1.1.11",
		"apollo-link-http": "^1.5.15",
		"apollo-link-ws": "^1.0.18",
		"babel-cli": "^6.26.0",
		"babel-eslint": "^8.0.2",
		"babel-loader": "^7.0.0",
		"better-npm-run": "^0.1.1",
		"expo": "^33.0.0",
		"graphql": "^14.4.1",
		"graphql-tag": "^2.10.1",
		"lodash": "^4.17.11",
		"native-base": "2.12.1",
		"react": "16.8.3",
		"react-apollo": "^2.5.8",
		"react-apollo-hooks": "^0.5.0",
		"react-dom": "^16.8.6",
		"react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
		"react-native-loading-spinner-overlay": "^1.0.1",
		"react-native-snap-carousel": "^3.8.0",
		"react-native-stars": "^1.1.5",
		"react-native-web": "^0.11.4",
		"react-navigation": "^3.11.0",
		"styled-components": "^4.3.2",
		"subscriptions-transport-ws": "^0.9.16"
	},
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"@babel/plugin-proposal-class-properties": "^7.3.0",
		"@babel/plugin-proposal-export-default-from": "^7.2.0",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-transform-flow-strip-types": "^7.2.3",
		"@babel/plugin-transform-runtime": "^7.2.0",
		"@storybook/addon-actions": "^4.1.6",
		"@storybook/addon-links": "^4.1.6",
		"@storybook/addon-notes": "^4.1.6",
		"@storybook/addon-ondevice-knobs": "^4.1.6",
		"@storybook/addon-ondevice-notes": "^4.1.6",
		"@storybook/addons": "^4.1.6",
		"@storybook/react-native": "^4.1.11",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^8.0.2",
		"babel-jest": "^23.2.0",
		"babel-plugin-jest-hoist": "^23.2.0",
		"babel-preset-expo": "^5.1.1",
		"concurrently": "^4.1.1",
		"eslint": "^4.19.1",
		"eslint-config-airbnb": "^15.1.0",
		"eslint-config-prettier": "^2.9.0",
		"eslint-config-standard": "^11.0.0",
		"eslint-config-standard-react": "^5.0.0",
		"eslint-plugin-babel": "^5.1.0",
		"eslint-plugin-import": "^2.2.0",
		"eslint-plugin-jsx-a11y": "^6.0.2",
		"eslint-plugin-node": "^6.0.1",
		"eslint-plugin-promise": "^3.4.0",
		"eslint-plugin-react": "^7.3.0",
		"eslint-plugin-standard": "^3.0.1",
		"plop": "^2.0.0",
		"prettier": "^1.18.2",
		"prop-types": "^15.7.2",
		"react-native-storybook-loader": "^1.8.0"
	},
	"private": true
}
